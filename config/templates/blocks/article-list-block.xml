<?xml version="1.0" ?>
<properties xmlns="http://schemas.sulu.io/template/template"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xi="http://www.w3.org/2001/XInclude"
            xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/template-1.0.xsd">
    <block name="content">
        <types>



            <type name="article-list-block">
                <meta>
                    <title lang="en">Article list block</title>
                </meta>
                <properties>
                    <property name="parent" type="single_page_selection" colspan="8">
                        <meta>
                            <title lang="en">Parent</title>
                        </meta>
                    </property>
                    <property name="template" type="single_select" colspan="4">
                        <meta><title lang="en">template</title></meta>
                        <tag name="sulu.block_preview" priority="128"/>
                        <params>
                            <param name="default_value" value="article"/>
                            <param name="values" type="collection">
                                <param name="article"><meta><title lang="en">article</title></meta></param>
                                <param name="default"><meta><title lang="en">default</title></meta></param>
                            </param>
                        </params>
                    </property>
                </properties>
            </type>



        </types>
    </block>
</properties>
